    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "prettier": "2.3.1",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0"
  }
}
